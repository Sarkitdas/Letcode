#include <iostream>
#include<vector>

using namespace std;

class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int minprice=prices[0];
        int maxprofit=0;
        for(int price:prices)
        {
            minprice=min(minprice,price);
            maxprofit=max(maxprofit,price-minprice);
        }
        return maxprofit;
    }
};

int main()
{
    Solution s;
    vector<int>price;
    price={7,1,5,3,6,4};
    cout<<s.maxProfit(price);
    return 0;
}
